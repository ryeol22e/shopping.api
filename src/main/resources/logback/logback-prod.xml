<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <!-- <property name="API_LOG" value="/users/logs/logback"/> -->
   <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
      <encoder class="ch.qos.logback.classic.encoder.PatternLayoutEncoder">
         <pattern>[%d{yyyy-MM-dd} %d{HH:mm:ss.SSS}][%p][%c:%L] - %m %n</pattern>
      </encoder>
   </appender>

   <!-- <logger name="org.springframework.core" value="info"/>
	<logger name="org.springframework.beans" value="info"/>
	<logger name="org.springframework.context" value="info"/>
	<logger name="org.springframework.web" value="info"/> -->

   <!-- <logger name="com.project.shopping">
		<level value="info" />
	</logger>

	<logger name="java.sql">
		<level value="debug" />
	</logger> -->

   <root level="info">
      <!-- <appender-ref ref="FILEOUT"/>  -->
      <appender-ref ref="STDOUT" />
   </root>
</configuration>